<%= simple_form_for(booking) do |f| %>
  <div class="form-inputs" >
    <%= f.input :f_inicio %>
    <%= f.input :f_termino %>
    <%= f.association :vehicle, collection: Vehicle.all.order(:model) %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-primary' %>
  </div>
<% end %>