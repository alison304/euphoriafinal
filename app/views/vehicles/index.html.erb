<div class="bookings container mt-large">
  <div class="card-columns">
    <% @vehicles.each do |vehicle| %>
          <div class="card" style="width: 18rem;">
            <% if vehicle.photo.key %>
              <%= cl_image_tag vehicle.photo.key, height: 200 %>
            <% end %>

            


            <div class="card-body">
              <h5 class="card-title"><%= vehicle.model %></h5>
              <p class="card-text">
                <%= vehicle.description %>
              </p>
              <%= link_to "Detalles", vehicle_path(vehicle) %>
            </div>
          </div>
    <% end %>
  </div>
</div>